{
  "name": "indicium-back",
  "description": "Indicium Backend",
  "version": "1.0.0",
  "author": "Thiago Cainelli",
  "private": true,
  "engines": {
    "node": ">=20.x"
  },
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node -r ./tsconfig-paths-bootstrap.js src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "generate": "prisma generate --schema=prisma/postgres/schema.prisma",
    "migrate:dev": "prisma migrate dev --schema=prisma/postgres/schema.prisma",
    "migrate:prod": "prisma migrate deploy --schema=prisma/postgres/schema.prisma",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "@prisma/extension-accelerate": "^2.0.1",
    "@types/express": "^5.0.3",
    "axios": "^1.10.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "fast-csv": "^5.0.5",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "path": "^0.12.7",
    "prisma": "^6.10.1",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^24.0.4",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "jest": "^29.7.0",
    "prettier": "^3.6.1",
    "prisma": "^6.10.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-jest": "^0.0.1",
    "typescript": "^5.8.3"
  }
}
